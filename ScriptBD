create table clienteendereco(
idusuario int,
idendereco int
);

create table endereco(
id int primary key generated always as identity,
titulo char(100),
cep char(8),
logradouro char(200),
numero int,
complemento char(100),
bairro char(100),
cidade char(100),
uf char(2)
);

create table imagens(
id int primary key generated always as identity,
dir char(100),
idproduto int,
capa boolean
);

create table produto(
id int primary key generated always as identity,
nomeproduto char(200),
nomeextenso char(1800),
estrelas int,
status boolean,
quantidade int,
preco double
);


create table tipocadastro(
id int primary key generated always as identity,
descricao char(50)
);


create table usuario(
id int primary key generated always as identity,
nome char(200),
senha char(300),
status boolean,
tipocadastro int,
email char(100),
cpf char(11)
);
